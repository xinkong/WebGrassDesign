<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.grass.mapper.ErrorMsgMapper">

    <select id="findErrorMsgByCode" parameterType="string" resultType="errorMsg">
        select * from t_error WHERE errorCode = #{errorCode};
    </select>

</mapper>